<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <directoryBrowse enabled="true" />
        <defaultDocument>
          <files>
            <clear />
            <add value="index.html" />
            <add value="index.php" />
            <add value="Default.htm" />
            <add value="Default.asp" />
            <add value="index.htm" />
            <add value="iisstart.htm" />
            <add value="default.aspx" />
          </files>
        </defaultDocument>
        <rewrite>
            <rules>

                <rule name="Rewrite-4" stopProcessing="true">
                    <match url="^beta/index\.html$" ignoreCase="false" />
                    <action type="None"/>
                </rule>

                <rule name="Rewrite-1" stopProcessing="true">
                    <match url="^beta/tjhzs_admin$" ignoreCase="false" />
                    <action type="Rewrite" url="/beta/server/index.html" />
                </rule>

                <rule name="Rule-3" stopProcessing="true">
                    <match url="^beta\/.*$" ignoreCase="false" />
                    <conditions logicalGrouping="MatchAll">
                        <add input="{REQUEST_FILENAME}" matchType="IsFile" ignoreCase="false" negate="true" />
                        <add input="{REQUEST_FILENAME}" matchType="IsDirectory" ignoreCase="false" negate="true" />
                    </conditions>
                    <action type="Rewrite" url="/beta/index.html" />
                </rule>

                <rule name="Rewrite-2" stopProcessing="true">
                    <match url="^beta/static\/$" ignoreCase="false" />
                    <action type="Rewrite" url="/beta/index.html" />
                </rule>
                <rule name="Rewrite-3" stopProcessing="true">
                    <match url="^beta/server$" ignoreCase="false" />
                    <action type="Rewrite" url="/beta/index.html" />
                </rule>

            </rules>
        </rewrite>
    </system.webServer>
</configuration>
